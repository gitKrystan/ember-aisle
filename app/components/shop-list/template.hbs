<ul class="list-unstyled">
  {{#each shops as |shop|}}
    <li>
      <button {{action 'newList' shop}} class="btn btn-link">
        <span class="glyphicon glyphicon-shopping-cart" aria-hidden="true"></span>
        Shop@ {{shop.brand.name}}: {{shop.name}}
      </button>
    </li>
  {{/each}}
</ul>
